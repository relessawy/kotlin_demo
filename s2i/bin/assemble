#!/bin/sh

set -e

source "/usr/libexec/s2i/logging.sh"
source "/usr/libexec/s2i/maven-s2i"

# include our s2i_core_*() overrides/extensions
source "/usr/libexec/s2i/s2i-core-hooks"

# inject our overridden maven_s2i_*() functions
source "/usr/libexec/s2i/maven-s2i-overrides"

# invoke the build
maven_s2i_build

